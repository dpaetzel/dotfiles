# msmtp config file

account gmail 
host smtp.gmail.com 
port 587 
protocol smtp 
auth on 
tls on 
tls_starttls on 
tls_trust_file /etc/ssl/certs/ca-certificates.crt 
from david.a.paetzel@gmail.com 
user david.a.paetzel@gmail.com 
passwordeval "pass mail/david.a.paetzel@gmail.com"


account default : gmail
